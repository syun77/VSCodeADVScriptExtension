{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "adv",
	"patterns": [{
		"include": "#keywords"
	}],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control",
					"match": "\\b(def|while|true|false|select|if|elif|else|call|return|break)\\b"
				}
			]
		}
	},
	"scopeName": "source.adv"
}